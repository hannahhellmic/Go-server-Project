package main

import (
	b "bufio"
	f "fmt"
	"os"
	"strconv"
	"strings"
)

func getAverage(arr []int) float64 {
	var total float64
	var cnt float64
	for _, el := range arr {
		total += float64(el)
		cnt++
	}
	return total / cnt
}

func main() {
	reader := b.NewReader(os.Stdin)
	s, _ := reader.ReadString('\n')
	s = strings.TrimSpace(s)
	a := strings.Split(s, " ")
	var arr []int
	for _, v := range a {
		var n, w = strconv.Atoi(v)
		if w == nil {
			arr = append(arr, n)
		}
	}

	f.Print(getAverage(arr))
}
